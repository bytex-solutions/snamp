<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<features xmlns="http://karaf.apache.org/xmlns/features/v1.4.0" name="snamp-web-console">
    <repository>mvn:com.bytex.snamp/platform/${snamp.version}/xml/features</repository>
    <feature name="snamp-web-console" description="${project.name}" version="${snamp.webconsole.version}">
        <feature version="[${snamp.version}, ${snamp.maxVersion})" prerequisite="true">snamp-core</feature>
        <feature version="[${snamp.version}, ${snamp.maxVersion})" prerequisite="true">snamp-web-support</feature>
        <feature version="[${snamp.version}, ${snamp.maxVersion})" prerequisite="false">snamp-management</feature>
        <bundle start="true">mvn:com.bytex.snamp/webconsole/${snamp.webconsole.version}</bundle>
    </feature>
</features>
