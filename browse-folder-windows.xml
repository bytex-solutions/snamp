<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns1:profile xmlns:ns1="http://snamp.bytex.solutions/schemas/command-line-tool-profile/v1.0">
    <ns1:reader>
        <ns1:output ns1:language="JavaScript" ns1:type="dictionary" >
            <ns1:entry ns1:key="free" ns1:type="string">scan.useDelimiter("\r\n").next().split(':')[1];</ns1:entry>
            <ns1:entry ns1:key="total" ns1:type="string">scan.useDelimiter("\r\n").next().split(':')[1];</ns1:entry>
            <ns1:entry ns1:key="available" ns1:type="string">scan.useDelimiter("\r\n").next().split(':')[1];</ns1:entry>
        </ns1:output>
        <ns1:input >fsutil volume diskfree {drive}</ns1:input>
    </ns1:reader>
</ns1:profile>
